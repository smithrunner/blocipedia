<div class="container text-center">
  <h3><%= @wiki.title %></h3><br>
  <div class="row">
    <div class="col-md-4"></div>
    <div class="col-md-4">
      <h4><%= @wiki.body %></h4>
    </div>
    <div class="col-md-4"></div>
    <div class="col-md-12">
      <%= link_to "Edit", edit_wiki_path(@wiki), class: 'btn btn-success' %>
      <%= link_to "Delete Wiki", [@wiki], method: :delete, class: 'btn btn-danger', data: {confirm: 'Are you sure you want do delete this wiki?'} %>
    </div>
  </div>
</div>
